extends layout

block content
  h1= title
  - var image_names = ["doremon-1", "doremon-2", "doremon-3", "doremon-4", "doremon-5"]
  - for(let i = 0; i < 3; i++) {
    - let number = Math.floor(Math.random() * 5)
        img(src=`/images/${image_names[number]}.png`, alt="different images every time", height="300px", width="300px")
  - }
  